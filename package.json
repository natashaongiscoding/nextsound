{
  "name": "nextsound",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "author": "NextWork",
  "scripts": {
    "dev": "vite",
    "dev:full": "concurrently \"npm run server:dev\" \"npm run dev\" --names \"server,client\" --prefix-colors \"blue,green\"",
    "server": "node server/index.js",
    "server:dev": "nodemon server/index.js",
    "build": "tsc && vite build",
    "build:server": "echo 'Server is plain JS, no build needed'",
    "preview": "vite preview",
    "start": "concurrently \"npm run server\" \"npm run preview\" --names \"server,client\" --prefix-colors \"blue,green\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:music": "./test-music-quality.sh",
    "test:visual": "playwright test test/visual-regression.spec.ts",
    "test:e2e": "playwright test",
    "validate:deploy": "npm run test && npm run build && echo 'âœ… Ready for deployment'",
    "quality:check": "npm run test:coverage && echo 'Quality validation complete'"
  },
